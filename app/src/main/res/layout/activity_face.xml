<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/advocate"
    tools:context=".face">

    <com.qmuiteam.qmui.widget.QMUIRadiusImageView
        android:id="@+id/head"
        android:layout_width="@dimen/d250"
        android:layout_height="@dimen/d250"
        android:layout_marginTop="@dimen/d72"
        android:scaleType="centerCrop"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.496"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView10"
        app:qmui_border_color="@color/coloronclice"
        app:qmui_border_width="2dp"
        app:qmui_is_circle="true"
        app:qmui_is_oval="true"
        app:qmui_selected_border_color="#ffffff"
        app:qmui_selected_border_width="2dp"
        app:qmui_selected_mask_color="@color/coloronclice" />

    <ImageView
        android:id="@+id/imageView10"
        android:layout_width="@dimen/d35"
        android:layout_height="@dimen/d35"
        android:layout_marginStart="@dimen/d4"
        android:layout_marginTop="@dimen/d8"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/returns" />

    <TextView
        android:id="@+id/textView10"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="@dimen/d16"
        android:text="添加人脸数据"
        android:textColor="@color/qmui_config_color_white"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/textView11"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginBottom="@dimen/d20"
        android:text="点击下方更换照片"
        android:textColor="@color/qmui_config_color_white"
        android:textSize="@dimen/s14"
        app:layout_constraintBottom_toTopOf="@+id/but"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent" />


    <ImageView
        android:id="@+id/ok"
        android:layout_width="@dimen/d30"
        android:layout_height="@dimen/d35"
        android:layout_marginTop="@dimen/d8"
        android:layout_marginEnd="@dimen/d8"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/ok" />


    <com.qmuiteam.qmui.widget.QMUIRadiusImageView
        android:id="@+id/but"
        android:layout_width="@dimen/d65"
        android:layout_height="@dimen/d65"
        android:scaleType="centerCrop"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:qmui_is_circle="true"
        app:qmui_is_oval="true"
        app:qmui_selected_mask_color="@color/coloronclice"
        app:srcCompat="@drawable/shoot"
        tools:layout_editor_absoluteY="639dp" />

</android.support.constraint.ConstraintLayout>