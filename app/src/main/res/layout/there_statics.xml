<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/background"
    tools:context=".there_statics">

    <View
        android:id="@+id/view7"
        android:layout_width="wrap_content"
        android:layout_height="@dimen/d50"
        android:background="@color/but"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/textView4"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="@dimen/d16"
        android:text="识别"
        android:textColor="@color/qmui_config_color_white"
        android:textSize="@dimen/s16"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <ImageView
        android:id="@+id/fold2"
        android:layout_width="@dimen/d35"
        android:layout_height="@dimen/d35"
        android:layout_marginStart="@dimen/d4"
        android:layout_marginTop="@dimen/d8"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/returns" />

    <com.qmuiteam.qmui.widget.QMUIRadiusImageView
        android:id="@+id/head"
        android:layout_width="@dimen/d180"
        android:layout_height="@dimen/d200"
        android:layout_marginStart="@dimen/d20"
        android:layout_marginTop="@dimen/d20"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/view7"
        app:qmui_border_color="@color/background"
        app:qmui_border_width="2dp"
        app:qmui_selected_border_color="#ffffff"
        app:qmui_selected_border_width="2dp"
        app:qmui_selected_mask_color="@color/background"
        app:srcCompat="@drawable/face_back" />

    <com.qmuiteam.qmui.widget.QMUIRadiusImageView
        android:id="@+id/heads"
        android:layout_width="@dimen/d180"
        android:layout_height="@dimen/d200"
        android:layout_marginTop="@dimen/d20"
        android:layout_marginEnd="@dimen/d20"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/view7"
        app:qmui_border_color="@color/background"
        app:qmui_border_width="2dp"
        app:qmui_selected_border_color="#ffffff"
        app:qmui_selected_border_width="2dp"
        app:qmui_selected_mask_color="@color/background"
        app:srcCompat="@drawable/face_back" />

    <ImageView
        android:id="@+id/imageView11"
        android:layout_width="@dimen/d40"
        android:layout_height="@dimen/d40"
        android:layout_marginBottom="@dimen/d5"
        app:layout_constraintBottom_toBottomOf="@+id/head"
        app:layout_constraintEnd_toEndOf="@+id/head"
        app:layout_constraintStart_toStartOf="@+id/head"
        app:srcCompat="@drawable/face_bancks" />

    <ImageView
        android:id="@+id/imageView12"
        android:layout_width="@dimen/d40"
        android:layout_height="@dimen/d40"
        android:layout_marginBottom="@dimen/d5"
        app:layout_constraintBottom_toBottomOf="@+id/heads"
        app:layout_constraintEnd_toEndOf="@+id/heads"
        app:layout_constraintStart_toStartOf="@+id/heads"
        app:srcCompat="@drawable/face_bancks" />

    <ImageView
        android:id="@+id/imageView8"
        android:layout_width="@dimen/d20"
        android:layout_height="@dimen/d20"
        app:layout_constraintBottom_toBottomOf="@+id/imageView12"
        app:layout_constraintEnd_toEndOf="@+id/imageView12"
        app:layout_constraintStart_toStartOf="@+id/imageView12"
        app:layout_constraintTop_toTopOf="@+id/imageView12"
        app:srcCompat="@drawable/upl" />

    <ImageView
        android:id="@+id/imageView13"
        android:layout_width="0dp"
        android:layout_height="@dimen/d50"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:srcCompat="@android:color/white" />

    <TextView
        android:id="@+id/textView8"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="@dimen/d8"
        android:text="对比"
        app:layout_constraintBottom_toBottomOf="@+id/imageView13"
        app:layout_constraintStart_toEndOf="@+id/imageView2"
        app:layout_constraintTop_toTopOf="@+id/imageView13" />

    <ImageView
        android:id="@+id/imageView2"
        android:layout_width="@dimen/d25"
        android:layout_height="@dimen/d25"
        android:layout_marginEnd="@dimen/d20"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="@+id/imageView13"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@+id/imageView13"
        app:srcCompat="@drawable/gen" />

    <ImageView
        android:id="@+id/imageView7"
        android:layout_width="@dimen/d20"
        android:layout_height="@dimen/d20"
        app:layout_constraintBottom_toBottomOf="@+id/imageView11"
        app:layout_constraintEnd_toEndOf="@+id/imageView11"
        app:layout_constraintStart_toStartOf="@+id/imageView11"
        app:layout_constraintTop_toTopOf="@+id/imageView11"
        app:srcCompat="@drawable/upl" />

    <ImageView
        android:id="@+id/imageView14"
        android:layout_width="0dp"
        android:layout_height="@dimen/d50"
        android:layout_marginStart="@dimen/d20"
        android:layout_marginEnd="@dimen/d20"
        app:layout_constraintBottom_toTopOf="@+id/imageView16"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:srcCompat="@drawable/back" />

    <ImageView
        android:id="@+id/imageView16"
        android:layout_width="0dp"
        android:layout_height="@dimen/d300"
        android:layout_marginBottom="@dimen/d20"
        app:layout_constraintBottom_toTopOf="@+id/imageView13"
        app:layout_constraintEnd_toEndOf="@+id/imageView14"
        app:layout_constraintStart_toStartOf="@+id/imageView14"
        app:srcCompat="@drawable/backs" />

    <TextView
        android:id="@+id/textView12"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="@dimen/d70"
        android:text="识别结果"
        android:textColor="@color/btn_ghost_blue_border_normal"
        app:layout_constraintBottom_toBottomOf="@+id/imageView14"
        app:layout_constraintStart_toStartOf="@+id/imageView14"
        app:layout_constraintTop_toTopOf="@+id/imageView14"
        app:layout_constraintVertical_bias="0.533" />

    <TextView
        android:id="@+id/textView13"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginEnd="@dimen/d70"
        android:text="算法介绍"
        android:textColor="@color/qmui_config_color_black"
        app:layout_constraintBottom_toBottomOf="@+id/imageView14"
        app:layout_constraintEnd_toEndOf="@+id/imageView14"
        app:layout_constraintTop_toTopOf="@+id/imageView14"
        app:layout_constraintVertical_bias="0.533" />

    <com.qmuiteam.qmui.widget.QMUIProgressBar
        android:id="@+id/progressBar"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:layout_marginStart="32dp"
        android:layout_marginTop="32dp"
        android:layout_marginEnd="32dp"
        android:layout_marginBottom="32dp"
        android:textDirection="firstStrongLtr"
        app:layout_constraintBottom_toBottomOf="@+id/imageView16"
        app:layout_constraintEnd_toEndOf="@+id/imageView16"
        app:layout_constraintStart_toStartOf="@+id/imageView16"
        app:layout_constraintTop_toTopOf="@+id/imageView16"
        app:qmui_background_color="@color/qmui_config_color_10_pure_black"
        app:qmui_progress_color="@color/qmui_config_color_blue"
        app:qmui_stroke_width="15dp"
        app:qmui_type="type_circle" />

    <TextView
        android:id="@+id/textView14"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="100%"
        android:textColor="@color/btn_ghost_blue_text_normal"
        android:textSize="@dimen/s24"
        app:layout_constraintBottom_toBottomOf="@+id/progressBar"
        app:layout_constraintEnd_toEndOf="@+id/progressBar"
        app:layout_constraintStart_toStartOf="@+id/progressBar"
        app:layout_constraintTop_toTopOf="@+id/progressBar" />

    <TextView
        android:id="@+id/textView15"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="相似度"
        app:layout_constraintEnd_toEndOf="@+id/progressBar"
        app:layout_constraintStart_toStartOf="@+id/progressBar"
        app:layout_constraintTop_toBottomOf="@+id/textView14" />

    <TextView
        android:id="@+id/textView16"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:layout_marginStart="16dp"
        android:layout_marginTop="16dp"
        android:layout_marginEnd="16dp"
        android:layout_marginBottom="16dp"
        android:singleLine="false"
        app:layout_constraintBottom_toBottomOf="@+id/imageView16"
        app:layout_constraintEnd_toEndOf="@+id/imageView16"
        app:layout_constraintStart_toStartOf="@+id/imageView16"
        app:layout_constraintTop_toTopOf="@+id/imageView16" />

</android.support.constraint.ConstraintLayout>